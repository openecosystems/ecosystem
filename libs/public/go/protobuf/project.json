{
  "name": "libs-public-go-protobuf-v2alpha",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "library",
  "sourceRoot": "libs/public/go/protobuf",
  "implicitDependencies": [
    "proto"
  ],
  "targets": {
    "generate": {
      "executor": "nx:run-commands",
      "options": {
        "command": "buf generate",
        "cwd": "libs/public/go/protobuf"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go build ./...",
        "cwd": "libs/public/go/protobuf"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go test -v ./... -cover -race",
        "cwd": "libs/public/go/protobuf"
      }
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "go clean",
        "cwd": "libs/public/go/protobuf"
      }
    },

    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["gofumpt -l .", "golangci-lint run ./..."],
        "parallel": false,
        "cwd": "libs/public/go/protobuf"
      }
    },
    "format": {
      "executor": "nx:run-commands",
      "options": {
        "commands": ["go mod tidy", "gofumpt -l -w .", "golangci-lint run ./... --fix"],
        "parallel": false,
        "cwd": "libs/public/go/protobuf"
      }
    }
  },
  "tags": []
}
