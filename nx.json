{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "packageManager": "pnpm"
  },
  "targetDefaults": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"],
      "cache": true
    },
    "generate": {
      "cache": true
    },
    "pack": {
      "dependsOn": ["build", "^pack"]
    },
    "container": {
      "dependsOn": ["build"]
    },
    "test": {
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
      "cache": true
    },
    "lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    },
    "eslint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    },
    "e2e": {
      "inputs": ["default", "^production"],
      "cache": true
    },
    "vulnerability": {
      "cache": true
    },
    "distribute": {
      "cache": true
    },
    "snapshot": {
      "cache": true
    },
    "nightly": {
      "cache": true
    },
    "build-storybook": {
      "inputs": [
        "default",
        "^production",
        "{projectRoot}/.storybook/**/*",
        "{projectRoot}/tsconfig.storybook.json"
      ],
      "cache": true
    },
    "pre-build": {
      "cache": true
    },
    "chromatic": {
      "cache": true
    },
    "@nx/jest:jest": {
      "cache": true,
      "inputs": ["default", "^production", "{workspaceRoot}/jest.preset.js"],
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/js:tsc": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"]
    }
  },
  "generators": {
    "@oeco/nx-react": {
      "component": {
        "style": "scss"
      }
    },
    "@nx/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "scss"
    },
    "@nx/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "bundler": "webpack",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint",
        "unitTestRunner": "jest"
      }
    },
    "@nx/node": {
      "application": {
        "standaloneConfig": true
      },
      "library": {
        "standaloneConfig": true
      }
    }
  },
  "defaultProject": "proto",
  "plugins": [
    "@nx-go/nx-go",
    "@openecosystems/nx-go",
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    }
  ],
  "pluginsConfig": {},
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [
      "{workspaceRoot}/babel.config.json",
      "{workspaceRoot}/go.work"
    ],
    "production": [
      "default",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/.storybook/**/*",
      "!{projectRoot}/**/*.stories.@(js|jsx|ts|tsx|mdx)",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/tsconfig.storybook.json"
    ]
  },
  "release": {
    "groups": {
      "sdks-public": {
        "projectsRelationship": "independent",
        "projects": ["tag:type:sdk", "!tag:cycle:private", "!tag:cycle:poc"],
        "git": {
          "commit": true,
          "tag": true,
          "commitMessage": "chore: updated version [no ci]"
        },
        "version": {
          "conventionalCommits": true,
          "generatorOptions": {
            "fallbackCurrentVersionResolver": "disk"
          }
        },
        "changelog": {
          "projectChangelogs": {
            "file": true,
            "createRelease": "github"
          }
        },
        "releaseTagPattern": "{projectName}/{version}"
      },
      "sdks-private": {
